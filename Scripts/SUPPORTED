-- loadstring stuff

local ARGS = {...}
local WINDOW = ARGS[1]
local NOTIFICATION = ARGS[2]

-- tabs

local SUPPORT_TAB = WINDOW:MakeTab({
   Name = "SUPPORTED",
   Icon = nil,
   PremiumOnly = false
})

-- services

local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")

-- locales

local Client = Players.LocalPlayer
local Games = {
 ["PANIK"] = 8511615377,
 ["SLAP ROYALE"] = 9426795465
}

-- setup

for i, v in pairs(Games) do
   SUPPORT_TAB:AddButton({
     Name = "[TELEPORT TO " .. tostring(i):upper() .. "]",
     Callback = function()
        NOTIFICATION("[TELEPORTING]", "HOLD ON!", 10, nil)
        TeleportService:Teleport(v)
     end
   })
end

-- connections

Client.OnTeleport:Connect(function(State)
    warn(State == Enum.TeleportState.Failed)
    if State == Enum.TeleportState.Failed then
       NOTIFICATION("[ERROR]", "CAN'T TELEPORT? DOES THIS GAME HAVE THIRD PARTY ENABLED?", 5, nil)
    end
end)
